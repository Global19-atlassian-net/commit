<% data.releases.each do |release| -%>
## <%= release.version %><%= release.value.include?("date") ? " ([#{release.date}](#{release.link}))" : "" %>

<% release.changes.each do |change| -%>
  *<%= change.value.include?("type") ? " `#{change.type}`" : "" %><%= change.value.include?("id") ? " [##{change.id}](#{change.link})" : "" %> <%= change.summary %><%= change.value.include?("author") ? " ([#{change.author.username}](#{change.author.link}))" : "" %>
<% end %>
<% end %>
